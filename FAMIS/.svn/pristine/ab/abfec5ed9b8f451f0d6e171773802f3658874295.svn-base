@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>自定义属性</title>
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />

    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="../../Scripts/Views/Dict/customAttributes.js"></script>



</head>
<body>
    <div class="easyui-layout" fit="true">
        <div id="p" data-options="region:'west'" title="资产类别列表" style="width: 300px; padding: 10px">
            <ul id="tree_assetType"  class="easyui-tree" data-options="border:false,singleSelect:true,fit:true,fitColumns:true,lines:true"></ul>
        </div>
        <div data-options="region:'center'" title="自定义属性" fit="true">
                <div  class="easyui-panel" data-options="region:'north'" title="当前属性" style="height:350px;">
                    <table id="datagrid_current" class="easyui-datagrid" fit="true" ></table>
                </div>
                <div  class="easyui-panel" data-options="region:'south',split:true" title="继承属性" style="height: 350px;" fit="true">

                    <table id="datagrid_inhert" class="easyui-datagrid" fit="true"></table>

                </div>
        </div>
         <div id="modalwindow" class="easyui-window" data-options="modal:true,closed:true,minimizable:false,shadow:false"></div>
    </div>
</body>
</html>
