@model IEnumerable<FAMIS.Models.Asset>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Asset_No)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asset_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asset_Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asset_User)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Variety)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Unit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Add_Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Arrange_Place)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_State)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Purchase_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_Supplier)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Assert_GB_Code)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Asset_No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asset_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asset_Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asset_User)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Variety)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Unit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Add_Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Arrange_Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Purchase_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_Supplier)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Assert_GB_Code)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
