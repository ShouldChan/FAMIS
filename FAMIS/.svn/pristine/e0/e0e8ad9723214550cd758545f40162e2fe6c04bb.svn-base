@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>

    <script type="text/javascript" src="../../Scripts/Views/Dict/add_supplier.js"></script>
</head>
<body>
    <div id="p" class="easyui-panel" fit="true" >
        <div style="text-align: center" >
            <table style="text-align: center; padding: 5px" align="center">
                <tr>
                    <td>供应商名称：</td>
                    <td>
                        <input class="easyui-textbox" id="GYSMC" style="width: 200px; height: 20px">
                    </td>
                </tr>
                <tr>
                    <td>联系人：</td>
                    <td>
                        <input class="easyui-textbox" id="LXR" style="width: 200px; height: 20px">
                    </td>
                </tr>
                <tr>
                    <td>联系电话：</td>
                    <td>
                        <input class="easyui-textbox" id="LXDH" style="width: 200px; height: 20px">
                    </td>
                </tr>
                <tr>
                    <td>邮箱：</td>
                    <td>
                        <input class="easyui-textbox" id="YX" style="width: 200px; height: 20px">
                    </td>
                </tr>
                <tr>
                    <td>传真：</td>
                    <td>
                        <input class="easyui-textbox" id="CZ" style="width: 200px; height: 20px">
                    </td>
                </tr>
                <tr>
                    <td>地址：</td>
                    <td>
                       <textarea id="DZ" rows=5 class="textarea easyui-validatebox"  style="width:200px;height:100px"> </textarea>
                    </td>
                </tr>
            </table>
        </div>
        <div style="text-align: center; padding: 5px">
            <a href="javascript:void(0)" class="easyui-linkbutton" onclick="submitForm()">提交</a>
            <a href="javascript:void(0)" class="easyui-linkbutton" onclick="cancelForm()">取消</a>
        </div>
    </div>
</body>
</html>
