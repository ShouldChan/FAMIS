@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>

    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />

    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="../../Scripts/Views/Asset/Asset_SubEquiment_add.js"></script>

</head>
<body>
    <div class="easyui-panel" style="padding: 5px;">
        <a href="#" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-save'" onclick="checkFormat('@ViewBag.id_asset')">提交</a>
        <table>
            <tr>
                <td style="width: 80px;">资产编号：</td>
                <td>
                    <input class="easyui-textbox" id="ZCBH_add" name="ZCBH_add" style="width: 146px;" />
                </td>
                <td>资产名称：</td>
                <td>
                    <input class="easyui-textbox" id="ZCMC_add" name="ZCMC_add" style="width: 146px;" />
                </td>
                <td>资产型号：</td>
                <td>
                    <input class="easyui-textbox" id="ZCXH_add" name="ZCXH_add" style="width: 146px;" />
                </td>
            </tr>
            <tr>
                <td>计量单位：</td>
                <td>
                    <select class="easyui-combobox" id="JLDW_add" name="JLDW_add" style="width: 150px;">
                    </select>
                </td>
                <td>价值：</td>
                <td>
                    <input class="easyui-numberbox" id="Other_ZCJZ_add" name="Other_ZCJZ_add" style="width: 146px;" />
                </td>
            </tr>
        </table>
        <table>
            <tr>
                <td style="width: 80px;">供应商：</td>
                <td>
                    <select class="easyui-combogrid" id="GYS_add" name="GYS_add" style="width: 340px;">
                    </select>
                </td>
            </tr>
            <tr>
                <td>备注：</td>
                <td>
                    <input class="easyui-textbox" id="note" name="note" style="width: 340px;" />
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
