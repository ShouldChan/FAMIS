  <script>
    $.ajax({

        type: "post",
        url: "/AssetDeatails/Get_Error_File",
       
        datatype: "json",//数据类型

        success: function (result) {

            if (result == "nofile") {
                $.ajax({

                    type: "post",
                    url: "/Depreciation/Set_ErroFile?Json=reset",

                    datatype: "json",//数据类型

                    success: function (result) {




                    }, error: function (msg) {
                        alert("Error");
                    }
                });
                
                $.messager.alert("提示", "请添加盘点数据Excel文件！", "error");
            }
            if (result == "wrongfile") {
                $.ajax({

                    type: "post",
                    url: "/Depreciation/Set_ErroFile?Json=reset",

                    datatype: "json",//数据类型

                    success: function (result) {

                       

                    }, error: function (msg) {
                        alert("Error");
                    }
                });
                $.messager.alert("提示", "您导入的文件格式不正确，请导入.xls或者.xlsx文件！", "error");

            }
            if (result == "wrongcode") {
                $.ajax({

                    type: "post",
                    url: "/Depreciation/Set_ErroFile?Json=reset",

                    datatype: "json",//数据类型

                    success: function (result) {



                    }, error: function (msg) {
                        alert("Error");
                    }
                });
                $.messager.alert("提示", "您导入的文件内容有误，请核对您的 ‘盘点数量’ 是否存在非法字符，如字母等！", "error");

            }
            if (result == "FileUploaded")
            {
                $.messager.alert("提示", "盘点数据导入成功！", "error");
                 
                $.ajax({

                    type: "post",
                    url: "/Depreciation/Set_ErroFile?Json=reset",
                   
                    datatype: "json",//数据类型

                    success: function (result) {


                        

                    }, error: function (msg) {
                        alert("Error");
                    }
                });

                
                window.parent.$('#filewindow').window('close');
            }

        }, error: function (msg) {
            alert("未能获得错误文件信息！");
        }
    });
      </script>






</head>


<body>
