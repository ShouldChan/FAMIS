@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>

    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/default/easyui.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/themes/icon.css" />
    <link rel="stylesheet" type="text/css" href="../../LIB/jquery-easyui-1.3.2/demo/demo.css" />

    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery-1.8.0.min.js"></script>
    <script type="text/javascript" src="../../LIB/jquery-easyui-1.3.2/jquery.easyui.min.js"></script>
    <script type="text/javascript" src="../../Scripts/UI/easyui-lang-zh_CN.js"></script>
    <script type="text/javascript" src="../../Scripts/Views/Asset/Asset_SubPicture_add.js"></script>
</head>
<body>
    <!--<form id="Form_document" method="post" action="/Asset/uploadDocument?id_asset=@ViewBag.id_asset" enctype="multipart/form-data" runat="server">
    </form>-->
    <iframe name='hidden_frame' id="hidden_frame" style='width:150px; height:50px; display:none;'> </iframe>
    <!--<form id="FORM1" action="/Asset/uploadDocument" target="hidden_frame" method="post" enctype="multipart/form-data" onsubmit="">
     
        </form>-->
     <table>
            <tr>
                <td style="width: 80px;">附属图片：</td>
                <td>
                    <input type="file" id="subFile" name="subFile"  style="width: 146px;"  />
                </td>
            </tr>
            <tr>
                <td>图片名称：</td>
                <td>
                    <input class="easyui-textbox" id="name" name="name" style="width: 146px;" />
                </td>
            </tr>
           <tr style="visibility:hidden">
                <td></td>
                <td>
                    <input  class="easyui-textbox" id="id_asset" name="id_asset" style="width: 146px;" value="@ViewBag.id_asset" />

                </td>
            </tr>
        </table>
        <div>
            <a href="#" class="easyui-linkbutton" onclick="checkForm('@ViewBag.id_asset')" data-options="iconCls:'icon-add'">提交</a>
            <!--<input type="submit" title="提交" value="提交" />-->
        </div>
</body>
</html>
